method: FontProgram
trailer
	"<String>"

	| zerosLine wst |
	zerosLine := String new: 64 withAll: $0.
	wst := String new writeStream.
	8 timesRepeat: [
		wst
			nextPutAll: zerosLine;
			cr].
	wst
		nextPutAll: 'cleartomark';
		cr.
	wst nextPutAll: self trailerExtra.
	^wst contents
%
method: FontProgram
