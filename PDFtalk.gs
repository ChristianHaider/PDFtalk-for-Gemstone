	^(self bitsPerRow / 8) ceiling
%
method: ImageXObject
clearMaskForBytes: numberOfBytes shiftedRigthBy: bitsRight
	| fullMask pixelMask |
	fullMask := (1 bitShift: numberOfBytes * 8) - 1.
	pixelMask := self pixelMask bitShift: bitsRight.
	^fullMask bitXor: pixelMask
