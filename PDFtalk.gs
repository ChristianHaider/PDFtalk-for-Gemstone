classmethod: Matrix
rotation: radians
	"<Matrix>
	rotate counter-clock wise.
	0 is to the right"

	| cos sin matrix |
	cos := radians cos.
	sin := radians sin.
	matrix := self identity.
	matrix at: 1 put: cos.
	matrix at: 2 put: sin.
	matrix at: 3 put: sin negated.
	matrix at: 4 put: cos.
	^matrix
