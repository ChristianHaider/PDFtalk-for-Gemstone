method: CrossReferenceSubsection
crossReferenceAt: aPDFReference ifAbsent: zeroArgumentBlock
	"<CrossReference>"

	| entry |
	(aPDFReference number between: self firstNumber and: self lastNumber) ifFalse: [
		^zeroArgumentBlock value].
	entry := self entries at: aPDFReference number - self firstNumber + 1.
	(entry matches: aPDFReference) ifTrue: [
