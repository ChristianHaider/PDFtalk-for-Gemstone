	| i |
	i := aCharacter asInteger.
	(aCharacter isPdfRegular and: [
	(i between: 16r21 and: 16r7E) and: [
	(aCharacter == $#) not]]) ifTrue: [
			^stream nextPut: i].
	stream nextPut: $# asInteger.
	stream nextPutAll: (aCharacter asInteger printStringRadix: 16) asByteArray
