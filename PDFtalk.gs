method: ImageXObject
pixelPaintForBits: anInteger
	"<ColorValue | CoverageValue>"

	self isMask ifTrue: [
		^self conversionColorspace
			coverageAt: anInteger
			withSizes: self depthsPerComponent
			decodeWith: self Decode asSmalltalkValue].
	^self conversionColorspace
		colorAt: anInteger
		withSizes: self depthsPerComponent
		decodeWith: self Decode asSmalltalkValue
