method: Type1Font
stringWidthOf: aString at: fontSize
	| stringGlyphs width |
	stringGlyphs := aString asOrderedCollection collect: [:char |
		self glyphAtUnicode: char asInteger].
	width := stringGlyphs inject: 0 into: [:sum :glyph |
		sum + (glyph ifNotNil: [glyph width] ifNil: [self unitsPerEm])].
	^width * fontSize / self unitsPerEm asFloat
