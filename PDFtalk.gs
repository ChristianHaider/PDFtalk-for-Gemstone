classmethod: FontProgram
encode: aByteArray key: keyInteger
	"<ByteArray>"

	| wst |
	wst := WriteStream on: (ByteArray new: aByteArray size).
	aByteArray inject: keyInteger into: [:r :byte |
		| cipher |
		cipher := byte bitXor: (r bitShift: -8).
		wst nextPut: cipher.
		self nextR: cipher + r].
	^wst contents
