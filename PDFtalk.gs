method: CrossReferences
freeReferences
	| ref freeReferences |
	ref := self atNumber: 0.
	freeReferences := OrderedCollection with: ref.
	[ref value isZero] whileFalse: [
		ref := self atNumber: ref value.
		freeReferences add: ref].
	^freeReferences
