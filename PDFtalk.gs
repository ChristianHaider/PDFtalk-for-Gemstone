category: 'accessing'
method: ContentParser
initialTokenActions
	^(Dictionary new)
		at: #null put: [nil];
		at: #true put: [true];
		at: #false put: [false];
		at: '(' put: [self readString];
		at: '<' put: [self readHexString];
		at: '/' put: [self readName];
		at: '[' put: [self readArray];
		at: '<<' put: [self readDictionary];
		yourself
