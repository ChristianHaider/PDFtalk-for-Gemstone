	pos := self position.
	[self atEnd] whileFalse: [
		char := self next.
		char = Character lf
			ifTrue: [
				^self]
			ifFalse: [
				char isPdfWhitespace ifFalse: [
					self errors add: (ReadError 
						newWith: pos 
						message: 'Read "' , char asSource , '". "<Character lf>" expected').
					^self position: pos]]].
