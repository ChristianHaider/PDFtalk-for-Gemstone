category: 'accessing'
classmethod: FontBody
decodings
	"<Dictionary key: Symbol value: Integer>
	a glyph name is associated with a unicode"

	^Decodings ifNil: [
		| dict |
		dict := IdentityDictionary new.
		(self adobeGlyphDecodingFrom: self adobeZapfdingbatsTxt readStream) keysAndValuesDo: [:glyphSymbol :unicode |
			dict at: glyphSymbol put: unicode].
		(self adobeGlyphDecodingFrom: self adobeGlyphlistTxt readStream) keysAndValuesDo: [:glyphSymbol :unicode |
			dict at: glyphSymbol put: unicode].
		Decodings := dict]
%
classmethod: FontBody
encodings
	"<Dictionary key: Integer value: (Array of: Symbol)>
	a unicode is associated with a list of glyph names"

	^Encodings ifNil: [
		| dict |
		dict := IdentityDictionary new.
		self decodings keysAndValuesDo: [:glyphSymbol :unicode |
			dict at: unicode put: ((dict at: unicode ifAbsent: [#()]) copyWith: glyphSymbol)].
		Encodings := dict]
