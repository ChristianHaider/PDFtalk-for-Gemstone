classmethod: CrossReferenceSection
readFrom: aParser
	"<CrossReferenceSection>"

	| subsections |
	subsections := OrderedCollection new.
	aParser expectToken: #xref.
	aParser skipWhitespace.
	[
	| char |
	char := aParser peek.
	char notNil and: [
	char isDigit]] whileTrue: [
