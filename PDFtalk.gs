method: DeviceGray
rgbBytesFrom: aByteArray
	"<ByteArray>"

	| wst rst |
	wst := (ByteArray new: aByteArray size * 3) writeStream.
	rst := aByteArray readStream.
	[rst atEnd] whileFalse: [
		| byte |
		byte := rst next.
		wst nextPut: byte.
		wst nextPut: byte.
		wst nextPut: byte].
	^wst contents
