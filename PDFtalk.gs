	| rst numberOfKernPairs |
	rst := aByteArray readStream.
	numberOfKernPairs := (ByteReadStream littleEndianOn: (rst next: 2)) nextUnsignedShort.
	^(1 to: numberOfKernPairs) collect: [:i | self fromBytes: (rst next: 4)]
