	| clearMask oldInteger |
	oldInteger := self bitsFromBytes: numberOfBytes at: bytesStart.
	clearMask := self clearMaskForBytes: numberOfBytes shiftedRigthBy: bitsRight.
	^oldInteger bitAnd: clearMask
