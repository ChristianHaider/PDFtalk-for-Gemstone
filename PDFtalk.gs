	"<PDF.String>
	replace EOL to LF"

	| rst wst |
	rst := aByteString readStream.
	wst := String new writeStream.
	[rst atEnd] whileFalse: [
		| char |
		char := rst next.
		char == Character cr ifTrue: [
			char := rst peek == Character lf
				ifTrue: [rst next]
				ifFalse: [Character lf]].
		wst nextPut: char].
	^wst contents
