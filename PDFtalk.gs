characterNameAtUnicode: anInteger
	"<Symbol>"

	anInteger = Character illegalCode ifTrue: [
		^#'.notdef'].
	^(self class characterNamesAtUnicode: anInteger)
		detect: [:symbol |
		self widths includesKey: symbol]
		ifNone: [
		#'.notdef']
