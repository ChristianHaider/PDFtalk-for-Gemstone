classmethod: CmykColor
gray: colorValueBrightness
	colorValueBrightness <= 0 ifTrue: [
		^self white].
	colorValueBrightness >= 1 ifTrue: [
		^self black].
	^self cyan: 0 magenta: 0 yellow: 0 black: colorValueBrightness
