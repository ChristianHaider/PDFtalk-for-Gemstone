category: 'instance creation'
classmethod: StandardFont
windowsName: nameString afm: afmString
	| inst |
	inst := self new.
	inst
		initializeWindowsFaceName: nameString
		metrics: (AdobeFontMetric fromAfm: afmString readStream).
	^inst
