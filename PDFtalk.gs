classmethod: Font
newFilename: fileString from: aBinaryReadStream
	| sfntVersion numTables header tables inst |
	sfntVersion := (aBinaryReadStream next: 4) asString.
	header := ByteReadStream bigEndianOn: (aBinaryReadStream next: 8).
	numTables := header nextUnsignedShort.
	inst := self
		filename: fileString
		sfntVersion: sfntVersion
		numTables: numTables
		searchRange: header nextUnsignedShort
		entrySelector: header nextUnsignedShort
		rangeShift: header nextUnsignedShort.
	tables := OrderedDictionary new.
	numTables timesRepeat: [
		| table |
		table := Table newFor: inst from: aBinaryReadStream.
		tables at: table tag put: table].
	inst tables: tables.
	^inst
