method: FontBody
encodeSymbolic: aCharacter
	| names |
	names := self class encodings at: aCharacter asInteger ifAbsent: [
		^nil].
	1 to: names size do: [:i |
		(self widths includesKey: (names at: i)) ifTrue: [
			^names at: i]].
